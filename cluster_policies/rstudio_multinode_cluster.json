{
    "spark_conf.spark.databricks.cluster.profile": {
      "type": "forbidden",
      "hidden": true
    },
    "runtime_engine": {
      "type": "fixed",
      "value": "STANDARD",
      "hidden": true
    },
    "spark_version": {
      "type": "regex",
      "pattern": "^(?!.*gpu).*ml.*",
      "defaultValue": "auto:latest-ml"
    },
    "autotermination_minutes": {
      "type": "unlimited",
      "defaultValue": 0,
      "isOptional": true
    },
    "node_type_id": {
      "type": "fixed",
      "value": "Standard_D16_v3",
      "hidden": true
    },
    "driver_node_type_id": {
      "type": "fixed",
      "value": "Standard_D16_v3",
      "hidden": true
    },
    "autoscale.min_workers": {
      "type": "fixed",
      "value": 1,
      "hidden": true
    },
    "autoscale.max_workers": {
      "type": "range",
      "maxValue": 6,
      "defaultValue": 2
    },
    "num_workers": {
      "type": "forbidden",
      "hidden": true
    },
    "azure_attributes.availability": {
      "type": "unlimited",
      "defaultValue": "ON_DEMAND_AZURE"
    },
    "azure_attributes.spot_bid_max_price": {
      "type": "fixed",
      "value": -1,
      "hidden": true
    },
    "instance_pool_id": {
      "type": "forbidden",
      "hidden": true
    },
    "driver_instance_pool_id": {
      "type": "forbidden",
      "hidden": true
    },
    "spark_conf.spark.databricks.delta.preview.enabled": {
      "type": "fixed",
      "value": "true",
      "hidden": false
    },
    "init_scripts.0.workspace.destination": {
      "type": "fixed",
      "value": "<DASH.sh location>"
    },
    "cluster_type": {
      "type": "fixed",
      "value": "all-purpose"
    },
    "custom_tags.WORKSPACE_NAME": {
      "type": "fixed",
      "value": "<WORKSPACE_NAME>"
    }
  }